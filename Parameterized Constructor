// Write a java program to illustrate parameterized constructor 
class demo2
{ int ID;
  String name;
  void display()
  { System.out.println("ID="+ID);
     System.out.println("Student Name="+name);
   }
   demo2(int i, String s)
    { ID=i;
      name=s;
    }
    public static void main(String args[])
     { demo2 d1=new demo2(111,"Sri");
       d1.display();
       demo2 d2=new demo2(222,"Karan");
       d2.display();
      }
}      
