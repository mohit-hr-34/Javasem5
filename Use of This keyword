class counterr
{ int rno;
  String name;
  float marks;
  counterr(int rno,String name,float marks)
  { this.rno=rno;
    this.name=name;
    this.marks=marks;
   }
   void display()
   { System.out.println("Roll No.="+rno);
     System.out.println("Student Name="+name);
     System.out.println("Marks="+marks);
   }
   public static void main(String args[])
   { counterr c1=new counterr(101,"SRI",309.1f);
            c1.display();
     }
}
OUTPUT:-Roll No.     =101
        Student Name =SRI
        Marks        =309.1
