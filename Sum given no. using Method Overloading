//Write a java program to find sum of given numbers using method overloading 
import java.util.*;
class addo
{ int add(int x, int y)
   { return(x+y);
    }
  float add(float x, float y)
    { return(x+y);
     }
   float add(int x, float y)
     { return(x+y);
      }
   int add(int x, int y, int z)
    { return(x+y+z);
     }
     public static void main(String args[])
     { 
       addo a1=new addo();
       System.out.println(a1.add(10,20));
       System.out.println(a1.add(10.55,20.57));
       System.out.println(a1.add(10,15,25));
       System.out.println(a1.add(10,11.57));
      }
}      
