//Write a program to find factorial of a given number
import java.util.*;
class facto
{ public static void main(String args[])
  { int num,i=1,fact=1;
     Scanner sc=new Scanner(System.in);
     System.out.println("Enter the Number");
     num=sc.nextInt();
     while(i<=num)
       { fact=i*fact;
          i=i+1;
        }
     System.out.println(num+"factorial is "+fact);
  }
}  
