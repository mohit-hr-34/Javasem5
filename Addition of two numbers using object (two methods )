//Write a java program to find addition of two numbers using object 
import java.util.*;
class ADDO
{ int a,b,c;
  void read()
  { Scanner sc=new Scanner(System.in);
    System.out.println("Enter two numbers ");
     a=sc.nextInt();
     b=sc.nextInt();
   }
   void sum()
   { c=a+b;
      System.out.println("The Addition is: "+c);
   }
   public static void main(String args[])
   { ADDO a1=new ADDO();
      a1.read();
      a1.sum();
    }
}    
